
# Solution
---------------------
#!/bin/python3

import sys

if __name__ == "__main__":
    meal_cost = float(input().strip())
    tip_percent = int(input().strip()) / 100
    tax_percent = int(input().strip()) / 100

totalCost = meal_cost + (meal_cost * tip_percent) + (meal_cost * tax_percent)
Roundtotal = round(totalCost)

print("The total meal cost is %d dollars." % Roundtotal)
